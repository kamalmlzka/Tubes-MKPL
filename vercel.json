{
    "builds": [
      { "src": "app/Views/index.php", "use": "vercel-php@0.3.6" },
      { "src": "public/**", "use": "@vercel/static" }
    ],
    "rewrites": [
      {
        "source": "/(css|js|images|assets)/(.*)",
        "destination": "/public/$1/$2"
      },
      {
        "source": "/(.*)",
        "destination": "/app/Views/index.php"
      }
    ]
  }
  